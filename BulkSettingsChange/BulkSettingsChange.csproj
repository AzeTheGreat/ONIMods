<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net35</TargetFramework>
    <AssemblyTitle>Settings Change Tool</AssemblyTitle>
    <FileVersion>1.8.0.0</FileVersion>
    <RootNamespace>PeterHan.BulkSettingsChange</RootNamespace>
    <Description>Adds a tool to enable/disable disinfect, auto repair, compost, empty storage, or building operation in bulk.</Description>
    <AssemblyVersion>1.7.0.0</AssemblyVersion>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Placer.dds" />
    <None Remove="Toggle.dds" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Placer.dds" />
    <EmbeddedResource Include="Toggle.dds" />
  </ItemGroup>
</Project>
